{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "December backend API server with CodeSandbox integration",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "npm run build && node --watch dist/index.js",
    "dev:bun": "bun --watch src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "prepare": "npm run build"
  },
  "dependencies": {
    "express": "^5.1.0",
    "openai": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "keywords": [
    "december",
    "ai",
    "codesandbox",
    "nextjs",
    "development",
    "api"
  ],
  "author": "December Team",
  "license": "MIT"
}